<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		id="sortDialog"
		confirm="_handleSortConfirm"
		sortItems="{tempModel>/sortItems}">
		<sortItems>
			<ViewSettingsItem text="{tempModel>name}" key="{tempModel>key}" selected="{tempModel>selected}"/>
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>